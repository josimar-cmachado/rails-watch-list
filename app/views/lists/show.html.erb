<div  class="container">
  <div class="row">
    <div class="col-12">
      <h1><%= @list.title %></h1>
      <ul>
        <% @list.movie.each do |movie| %>
          <li><%= movie.title %></li>
        <% end %>
      </ul>
      <%= link_to "Add a movie" , new_list_movie_path(@list) %>
    </div>
  </div>
</div>
